#searchWrap fieldset input.btn_service { background:url("http://c1img.cyworld.co.kr/img/error/set_search.gif") no-repeat 0 0; border:0;  cursor:pointer; float:left; height:25px; margin:0; padding:0; width:42px; }
</style>
<script type="text/javascript">
	function SearchGo()
	{
		_qu = document.search.q.value;

		if(_qu == "" )
		{
			alert("검색어를 입력해 주세요.");
			return false;
		}
		
		document.search.submit();
		return true;
	}
</script>
