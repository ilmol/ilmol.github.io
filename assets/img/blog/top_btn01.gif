a:hover { text-decoration:underline; }
 
.error_wrap { position:relative; margin:75px auto 0 auto; width:577px; height:418px; background:url('http://c1img.cyworld.co.kr/img/error/error_cyworld_n_full_cy.gif') no-repeat left top; }
